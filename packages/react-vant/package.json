{
  "name": "react-vant",
  "version": "1.3.1",
  "description": "React Mobile UI Components base on Vant UI",
  "repository": {
    "type": "git",
    "url": "git@github.com:3lang3/react-vant.git"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "style": "lib/index.css",
  "typings": "lib/index.d.ts",
  "unpkg": "lib/react-vant.min.js",
  "jsdelivr": "lib/react-vant.min.js",
  "files": [
    "lib",
    "es"
  ],
  "scripts": {
    "start": "react-vant-cli dev",
    "create-component": "react-vant-cli create-component",
    "postcreate-component": "yarn prettier",
    "prettier": "npx prettier --write vant.config.js src/index.ts",
    "build": "react-vant-cli build",
    "changelog": "react-vant-cli changelog",
    "build:site": "react-vant-cli build-site",
    "release:site": "npm run build:site && gh-pages -d site",
    "prepack": "yarn build",
    "test": "react-vant-cli test --coverage",
    "test:coverage": "open tests/coverage/index.html"
  },
  "author": "3lang3",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "ui",
    "react",
    "mobile",
    "components"
  ],
  "devDependencies": {
    "@react-vant/cli": "^1.1.15",
    "@types/classnames": "^2.2.11",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@vant/area-data": "^1.1.3",
    "gh-pages": "^3.2.3",
    "rc-field-form": "^1.21.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 8"
  ],
  "dependencies": {
    "@react-spring/web": "^9.3.0",
    "@use-gesture/react": "^10.1.1",
    "@vant/icons": "^1.7.0",
    "@vant/popperjs": "^1.1.0",
    "classnames": "^2.2.6",
    "rc-field-form": "^1.21.2",
    "react-transition-group": "^4.4.1",
    "swiper": "^6.8.1"
  }
}
